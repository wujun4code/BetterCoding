<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>10.0</LangVersion>
    <PackageVersion>0.0.6</PackageVersion>
    <Title>Design patterns hosted by Better Coding</Title>
    <Authors>Better Coding@wujun4code</Authors>
    <Description>Provides design patterns in .NET.</Description>
    <Copyright>Better Coding@wujun4code</Copyright>
    <PackageProjectUrl>https://github.com/wujun4code/BetterCoding</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/wujun4code/BetterCoding</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

</Project>
